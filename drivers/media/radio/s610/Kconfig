#
# SAMSUNG's S6XX FM driver based on SPEEDY driver.
#
menu "Samsung S6XX FM driver (SPEEDY based)"
config RADIO_S610
	tristate "Samsung S610 FM Radio"
	depends on VIDEO_V4L2 && (RADIO_S5E3830 || RADIO_S5E7885 || RADIO_S5E9610 || RADIO_S5E9630)
	help
	Choose Y here if you have this FM radio chip.

config RADIO_S621
	tristate "Samsung S621 FM Radio"
	depends on VIDEO_V4L2 && (RADIO_S5E3830 || RADIO_S5E7885 || RADIO_S5E9610 || RADIO_S5E9630)
	help
	Choose Y here if you have this FM radio chip.

endmenu
